Phase: DegreeFinder
Input: Token Identifier DEGREE SpaceToken
Options: control = appelt debug = false

Rule: DegreeRule1
Priority: 100
(
  {Identifier.majorType == degree}
  ({Token.category == PU})?
  ({SpaceToken})[0,2]
  (
    {DEGREE}
  ):degreeLookup
):section
-->
:degreeLookup.DEGREE = {conf = strong},
:section.Section = {kind = degree}

Rule: DegreeRule2
Priority: 50
{SpaceToken}
(
	{DEGREE}
)
:degreeLookup
{SpaceToken}
-->
:degreeLookup.DEGREE = {conf = normal},
:degreeLookup.Section = {kind = degree}

Rule: DegreeRule3
Priority: 20
{SpaceToken}
(
	{DEGREE}
)
:degreeLookup
{SpaceToken}
-->
:degreeLookup.DEGREE = {conf = weak},
:degreeLookup.Section = {kind = degree}